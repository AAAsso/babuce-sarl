{% extends 'base.html.twig' %}

{% block body %}
    <h1>Contentwarning</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contentWarning.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ contentWarning.label }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ contentWarning.description }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('contentwarning_list') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('contentwarning_edit', { 'label': contentWarning.label }) }}">Edit</a>
        </li>
    </ul>
{% endblock %}
